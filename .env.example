# Function Cloud Environment Variables
# Copy this file to .env and fill in your values
# DO NOT commit the .env file to version control

# Modal credentials
MODAL_TOKEN_ID=
MODAL_TOKEN_SECRET=

# Groq API key for LLM integration
GROQ_API_KEY=

# Google OAuth credentials (for web interface)
FC_GOOGLE_CLIENT_ID=
FC_GOOGLE_CLIENT_SECRET=

# Flask secret key (generate with: python -c "import os; print(os.urandom(24).hex())")
FC_SECRET_KEY=

# Supabase credentials (if using Supabase)
SUPABASE_URL=
SUPABASE_KEY=

# Function Cloud configuration
FC_ENVIRONMENT=development  # development, staging, production
FC_REGION=us-east
FC_MEMORY=1024
FC_TIMEOUT=60
FC_CPU=1
FC_LLM_ENABLED=true
FC_LLM_PROVIDER=groq
FC_STUB_NAME=fc_app
